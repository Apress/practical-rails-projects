  <% @page_heading = "Trackbacks for #{ link_to h(@article.title), :controller => 'content', :action => 'show', :id => @article }" %>

  <%= start_form_tag :action=>"edit", :id => @trackback.id %>

  <div class="form">	
    <h3>Editing trackback</h3>
    <%= render_partial "form" %>
  </div>

  <div id="operations">
    <%= save %> or <%= cancel %>  
  </div>

  <%= end_form_tag %>
